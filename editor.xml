<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Hack" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="Config" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JSON" value="true" />
        <entry key="MSBuild" value="true" />
        <entry key="Resx" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="IS_CARET_BLINKING" value="false" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\bNotImplementedException\b" />
    <pattern icon="question" useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;NOTE)(\W|$)(.*)">
      <option name="FOREGROUND" value="c3ff" />
      <option name="EFFECT_COLOR" value="c3ff" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)">
      <option name="FOREGROUND" value="ffff00" />
      <option name="BACKGROUND" value="0" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="0" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="ff0000" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="ff0000" />
      <option name="ERROR_STRIPE_COLOR" value="ff0000" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
    <filter name="Bugs Only">
      <pattern index="3" />
    </filter>
    <filter name="TODO Only">
      <pattern index="2" />
    </filter>
    <filter name="Notes Only">
      <pattern index="1" />
    </filter>
  </component>
</application>